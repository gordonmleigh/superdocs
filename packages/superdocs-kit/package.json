{
  "name": "@gordonmleigh/superdocs-kit",
  "version": "0.1.10",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Gordon Leigh"
  },
  "repository": "https://github.com/gordonmleigh/superdocs",
  "homepage": "https://gordonmleigh.github.io/superdocs",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ --ext=ts,tsx"
  },
  "exports": {
    "./components/*": {
      "types": "./lib/components/*.d.ts",
      "default": "./lib/components/*.js"
    },
    "./content/*": {
      "types": "./lib/content/*.d.ts",
      "default": "./lib/content/*.js"
    },
    "./tailwind": {
      "types": "./lib/tailwind-plugin.d.ts",
      "default": "./lib/tailwind-plugin.js"
    },
    "./util/*": {
      "types": "./lib/util/*.d.ts",
      "default": "./lib/util/*.js"
    }
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "!lib/**/*.test.*",
    "!lib/testing/",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "static/**"
  ],
  "dependencies": {
    "@gordonmleigh/superdocs": "^0.1.10",
    "@heroicons/react": "^2.0.18",
    "fast-glob": "^3.3.1",
    "next-mdx-remote": "^4.4.1"
  },
  "peerDependencies": {
    "clsx": "*",
    "next": "^13.4",
    "react": "^18",
    "tailwindcss": "^3",
    "typescript": "*"
  }
}
