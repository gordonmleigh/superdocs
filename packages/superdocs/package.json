{
  "name": "superdocs",
  "version": "0.1.0",
  "dependencies": {
    "chalk": "^5.3.0",
    "convert-source-map": "^2.0.0",
    "source-map": "^0.6.1"
  },
  "scripts": {
    "lint": "eslint src/ --ext=ts,tsx"
  },
  "exports": {
    "./components/*": {
      "types": "./lib/components/*.d.ts",
      "default": "./lib/components/*.js"
    },
    "./components/ast/*": {
      "types": "./lib/components/ast/*.d.ts",
      "default": "./lib/components/ast/*.js"
    },
    "./components/declarations/*": {
      "types": "./lib/components/declarations/*.d.ts",
      "default": "./lib/components/declarations/*.js"
    },
    "./core/*": {
      "types": "./lib/core/*.d.ts",
      "default": "./lib/core/*.js"
    },
    "./tailwind": {
      "default": "./lib/tailwind-plugin.js"
    }
  }
}
